{
  "$schema": "https://json.schemastore.org/prettierrc",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "printWidth": 150,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 4,
  "useTabs": false,
  "vueIndentScriptAndStyle": true,
  "trailingComma": "none",
  "arrowParens": "avoid"
}
